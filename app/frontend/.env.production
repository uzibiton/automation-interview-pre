# Enable/Disable Mock API for production build
# Set to 'true' to use Mock Service Worker (MSW) for API requests
# Set to 'false' to use real backend API
VITE_USE_MOCK_API=false

# API Base URL (used when mock API is disabled)
# This should be overridden by deployment pipeline with actual production URL
VITE_API_BASE_URL=https://api.example.com
